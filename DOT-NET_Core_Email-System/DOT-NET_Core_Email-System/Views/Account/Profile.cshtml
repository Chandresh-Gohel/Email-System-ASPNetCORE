@model DOT_NET_Core_Email_System.Models.DbUser

@{
    ViewData["Title"] = "Profile";
}

<h1>Profile</h1>

<div>
    <h4>SignUpViewModel</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            User Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class = "col-sm-2">
            First Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserFirstName)
        </dd>
        <dt class = "col-sm-2">
            Last Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserLastName)
        </dd>
        <dt class = "col-sm-2">
            Date of Birth
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DOB)
        </dd>
    </dl>
</div>
<div>
    <a href="UpdateProfile">Update Profile</a>
</div>
